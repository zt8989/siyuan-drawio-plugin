{
  "name": "plugin-sample-vite",
  "version": "0.1.4",
  "type": "module",
  "description": "This is a sample plugin based on vite and svelte for Siyuan (https://b3log.org/siyuan)",
  "repository": "https://github.com/frostime/plugin-sample-vite",
  "homepage": "",
  "author": "frostime",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development VITE_SOURCEMAP=inline vite build --watch",
    "prebuild": "sh -c ./scripts/copy_webapp_build.sh",
    "build": "cross-env NODE_ENV=production vite build",
    "postbuild": "sh -c ./scripts/copy_and_bundle_build.sh",
    "prebuild-win": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File ./scripts/copy_webapp_build.ps1",
    "build-win": "pnpm run build",
    "postbuild-win": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File ./scripts/copy_and_bundle_build.ps1",
    "make-link": "node --no-warnings ./scripts/make_dev_link.js",
    "make-build-link": "node --no-warnings ./scripts/make_build_link.js",
    "make-link-win": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File ./scripts/elevate.ps1 -scriptPath ./scripts/make_dev_link.js",
    "make-install": "pnpm run build && node --no-warnings ./scripts/make_install.js",
    "update-version": "node --no-warnings ./scripts/update_version.js"
  },
  "devDependencies": {
    "@types/node": "^20.3.0",
    "cross-env": "^7.0.3",
    "fast-glob": "^3.2.12",
    "glob": "^10.0.0",
    "js-yaml": "^4.1.0",
    "minimist": "^1.2.8",
    "rollup-plugin-livereload": "^2.0.5",
    "sass": "^1.63.3",
    "siyuan": "1.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^5.2.9",
    "vite-plugin-static-copy": "^1.0.2",
    "vite-plugin-zip-pack": "^1.0.5"
  },
  "dependencies": {
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0",
    "workbox-window": "^7.1.0"
  }
}